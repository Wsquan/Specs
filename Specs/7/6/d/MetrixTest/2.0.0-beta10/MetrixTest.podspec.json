{
  "name": "MetrixTest",
  "version": "2.0.0-beta10",
  "summary": "This is the iOS SDK of Metrix.",
  "description": "Metrix is an marketing attribution & analytics framework for mobile. You can read more about it at https://metrix.ir.",
  "homepage": "https://metrix.ir",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Matin": "m.zareimatin@gmail.com"
  },
  "platforms": {
    "ios": "10.0"
  },
  "source": {
    "http": "https://git.tapsell.ir/m.zareimatin/ios-sdk-publish/-/raw/master/MetrixFramework.zip"
  },
  "ios": {
    "vendored_frameworks": "Metrix.framework"
  },
  "frameworks": [
    "UIKit",
    "AdSupport",
    "CoreTelephony",
    "SystemConfiguration",
    "CoreLocation"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": ""
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": ""
  },
  "default_subspecs": "native",
  "subspecs": [
    {
      "name": "native",
      "ios": {
        "vendored_frameworks": "Metrix.framework"
      }
    },
    {
      "name": "reactnative",
      "ios": {
        "vendored_frameworks": "Metrix_reactnative.framework"
      }
    },
    {
      "name": "unity",
      "ios": {
        "vendored_frameworks": "Metrix_unity.framework"
      }
    },
    {
      "name": "flutter",
      "ios": {
        "vendored_frameworks": "Metrix_flutter.framework"
      }
    },
    {
      "name": "cordova",
      "ios": {
        "vendored_frameworks": "Metrix_cordova.framework"
      }
    }
  ]
}
